{
  "cli": {
    "version": ">= 4.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.19.4",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "NODE_ENV": "development"
      }
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest",
        "simulator": true
      },
      "channel": "development"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "NODE_ENV": "preview"
      },
      "channel": "preview"
    },
    "production": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production"
      },
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
